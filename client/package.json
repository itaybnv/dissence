{
	"name": "client",
	"version": "0.1.0",
	"main": "public/electron.js",
	"private": true,
	"dependencies": {
		"electron-is-dev": "^1.1.0",
		"node-sass": "^4.13.0",
		"react": "^16.11.0",
		"react-dom": "^16.11.0",
		"react-scripts": "3.2.0",
		"rmwc": "^5.7.1"
	},
	"scripts": {
		"react-start": "react-scripts start",
		"react-build": "react-scripts build",
