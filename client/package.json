		"electron-pack": "electron-builder",
		"build": "npm run react-build && npm run electron-pack"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		]
	},
	"devDependencies": {
		"concurrently": "^5.0.0",
		"wait-on": "^3.3.0",
		"electron": "^7.1.1",
		"electron-builder": "^22.1.0"
	}
}
